{
  "ls": [
    "if [ -z \"$arg\" ]",
    "then ls -lah",
    "else eval val=\\$$arg; ls -lah $val; fi"
  ],
  "env": [
    "newdef=`cat define.$arg.json`",
    "if [ -n \"$newdef\" ]",
    "then cp define.$arg.json define.json; fi"
  ],
  "help": [
    "printf \"\n`cat README*`\n\" | less -F"
  ],
  "echo": [
    "eval val=\\$$arg",
    "if [ -n \"$val\" ]",
    "then echo $val; fi"
  ]
}